{
  "name": "axis-discovery-ssdp",
  "version": "1.0.1",
  "description": "A node.js SSDP client library written in TypeScript capable of searching for Axis Communication cameras.",
  "keywords": [
    "axis",
    "communications",
    "camera",
    "device",
    "discovery",
    "ssdp",
    "upnp"
  ],
  "homepage": "https://github.com/FantasticFiasco/axis-discovery-ssdp",
  "repository": {
    "type": "git",
    "url": "https://github.com/FantasticFiasco/axis-discovery-ssdp.git"
  },
  "bugs": {
    "url": "https://github.com/FantasticFiasco/axis-discovery-ssdp/issues"
  },
  "author": "Mattias Kindborg",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "types": "./lib/index",
  "engines": {
    "node": ">=4.6.1"
  },
  "scripts": {
    "clean:src": "rimraf ./src/**/*.js ./src/**/*.d.ts ./src/**/*.js.map",
    "clean:test": "rimraf ./test/**/*.js ./test/**/*.d.ts ./test/**/*.js.map",
    "clean:lib": "rimraf ./lib/",
    "clean": "npm run clean:src && npm run clean:test && npm run clean:lib",
    "prebuild": "npm run clean",
    "build": "tsc",
    "postbuild": "cpx ./src/**/*.{d.ts,js} ./lib/",
    "prestart": "npm run build",
    "start": "node ./src/server.js",
    "pretest": "npm run build",
    "test": "mocha --recursive",
    "lint": "tslint ./src/**/*.ts --exclude ./**/*.d.ts",
    "prepack": "npm run test && npm run lint",
    "pack": "npm pack"
  },
  "dependencies": {
    "bluebird": "3.4.7",
    "lodash": "4.17.4",
    "request": "2.79.0",
    "request-promise": "4.1.1",
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "@types/bluebird": "3.0.37",
    "@types/chai": "3.4.34",
    "@types/chai-as-promised": "0.0.29",
    "@types/lodash": "4.14.48",
    "@types/mocha": "2.2.36",
    "@types/node": "6.0.59",
    "@types/request": "0.0.38",
    "@types/request-promise": "4.1.33",
    "@types/xml2js": "0.0.30",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "cpx": "1.5.0",
    "mocha": "3.2.0",
    "rimraf": "2.5.4",
    "tslint": "4.3.1",
    "typescript": "2.1.4"
  }
}
